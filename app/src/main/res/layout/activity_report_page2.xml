<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context=".ReportPage2">

  <TextView
      android:id="@+id/textView8"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Time"
      android:textColor="#F44336"
      android:textSize="17sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.042"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView7"
      app:layout_constraintVertical_bias="0.375" />

  <CheckBox
      android:id="@+id/c5"
      android:layout_width="107dp"
      android:layout_height="45dp"
      android:text="Abuse"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.498"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/c2"
      app:layout_constraintVertical_bias="0.051" />

  <CheckBox
      android:id="@+id/c2"
      android:layout_width="115dp"
      android:layout_height="51dp"
      android:text="Harresment"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.498"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView5"
      app:layout_constraintVertical_bias="0.038" />

  <CheckBox
      android:id="@+id/c4"
      android:layout_width="91dp"
      android:layout_height="47dp"
      android:text="Rape"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toStartOf="@+id/c5"
      app:layout_constraintHorizontal_bias="0.104"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/c1"
      app:layout_constraintVertical_bias="0.053" />

  <TextView
      android:id="@+id/textView5"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="casual"
      android:text="Incident"
      android:textColor="#F44336"
      android:textSize="34sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.05" />

  <CheckBox
      android:id="@+id/c1"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Robbery"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toStartOf="@+id/c2"
      app:layout_constraintHorizontal_bias="0.164"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView5"
      app:layout_constraintVertical_bias="0.038" />

  <CheckBox
      android:id="@+id/c6"
      android:layout_width="77dp"
      android:layout_height="47dp"
      android:text="Other"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.875"
      app:layout_constraintStart_toEndOf="@+id/c5"
      app:layout_constraintTop_toBottomOf="@+id/c3"
      app:layout_constraintVertical_bias="0.051" />

  <CheckBox
      android:id="@+id/c3"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Accident"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.909"
      app:layout_constraintStart_toEndOf="@+id/c2"
      app:layout_constraintTop_toBottomOf="@+id/textView5"
      app:layout_constraintVertical_bias="0.038" />

  <EditText
      android:id="@+id/et_other"
      android:layout_width="match_parent"
      android:layout_height="51dp"
      android:editable="false"
      android:ems="10"
      android:hint="Other"
      android:inputType="textPersonName"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/c5"
      app:layout_constraintVertical_bias="0.042" />

  <TextView
      android:id="@+id/textView6"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Gender"
      android:textColor="#F44336"
      android:textSize="17sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.044"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/et_other"
      app:layout_constraintVertical_bias="0.081" />

  <TextView
      android:id="@+id/textView7"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Age"
      android:textColor="#F44336"
      android:textSize="17sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.062"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView6"
      app:layout_constraintVertical_bias="0.179" />

  <RadioGroup
      android:id="@+id/rg_gen"
      android:layout_width="310dp"
      android:layout_height="50dp"
      android:orientation="horizontal"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.23"
      app:layout_constraintStart_toEndOf="@+id/textView6"
      app:layout_constraintTop_toBottomOf="@+id/et_other"
      app:layout_constraintVertical_bias="0.053">

    <RadioButton
        android:id="@+id/m"
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:text="Male" />

    <RadioButton
        android:id="@+id/f"
        android:layout_width="109dp"
        android:layout_height="wrap_content"
        android:text="Female" />

    <RadioButton
        android:id="@+id/o"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Other" />
  </RadioGroup>

  <Button
      android:id="@+id/btn_report"
      android:layout_width="99dp"
      android:layout_height="52dp"
      android:background="#F44336"
      android:text="Report"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.523"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/rg_time"
      app:layout_constraintVertical_bias="0.541" />

  <RadioGroup
      android:id="@+id/rg_age"
      android:layout_width="322dp"
      android:layout_height="75dp"
      android:layout_marginTop="20dp"

      android:orientation="horizontal"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.485"
      app:layout_constraintStart_toEndOf="@+id/textView7"
      app:layout_constraintTop_toBottomOf="@+id/rg_gen"
      app:layout_constraintVertical_bias="0.03">

    <RadioButton
        android:id="@+id/a1"
        android:layout_width="81dp"
        android:layout_height="58dp"
        android:text="0-18" />

    <RadioButton
        android:id="@+id/a2"
        android:layout_width="80dp"
        android:layout_height="62dp"
        android:text="19-45" />

    <RadioButton
        android:id="@+id/a3"
        android:layout_width="82dp"
        android:layout_height="57dp"
        android:text="46-60" />

    <RadioButton
        android:id="@+id/a4"
        android:layout_width="74dp"
        android:layout_height="59dp"
        android:text="60+" />

  </RadioGroup>

  <RadioGroup
      android:id="@+id/rg_time"
      android:layout_width="348dp"
      android:layout_height="121dp"
      android:orientation="vertical"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.666"
      app:layout_constraintStart_toEndOf="@+id/textView8"
      app:layout_constraintTop_toBottomOf="@+id/rg_age"
      app:layout_constraintVertical_bias="0.216">

    <TableRow>

      <RadioButton
          android:id="@+id/t1"
          android:layout_width="108dp"
          android:layout_height="51dp"
          android:text="00:00-4:00" />

      <RadioButton
          android:id="@+id/t2"
          android:layout_width="119dp"
          android:layout_height="55dp"
          android:text="4:00-8:00" />

      <RadioButton
          android:id="@+id/t3"
          android:layout_width="111dp"
          android:layout_height="50dp"
          android:text="8:00-12:00" />
    </TableRow>

    <TableRow>

      <RadioButton
          android:id="@+id/t4"
          android:layout_width="112dp"
          android:layout_height="52dp"
          android:text="12:00-16:00" />

      <RadioButton
          android:id="@+id/t5"
          android:layout_width="114dp"
          android:layout_height="51dp"
          android:text="16:00-20:00" />

      <RadioButton
          android:id="@+id/t6"
          android:layout_width="115dp"
          android:layout_height="52dp"
          android:text="20:00-00:00" />
    </TableRow>

  </RadioGroup>

</androidx.constraintlayout.widget.ConstraintLayout>